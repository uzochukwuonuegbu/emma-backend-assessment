{
  "name": "emma-backend-assessment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "npm run prestart:prod && node dist/src/main.js",
    "start": "nodemon",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migrate:create": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:rollback:all": "knex migrate:rollback --all",
    "migrate:up": "npm run prestart:prod && knex migrate:up",
    "migrate:down": "knex migrate:down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzochukwuonuegbu/emma-backend-assessment.git"
  },
  "author": "Uzochukwu Onuegbu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uzochukwuonuegbu/emma-backend-assessment/issues"
  },
  "homepage": "https://github.com/uzochukwuonuegbu/emma-backend-assessment#readme",
  "dependencies": {
    "agenda": "^3.0.0",
    "argon2": "0.27.0",
    "body-parser": "1.19.0",
    "celebrate": "15.0.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "helmet": "4.6.0",
    "joi": "^17.4.1",
    "jsonwebtoken": "8.5.1",
    "knex": "^0.95.14",
    "mongodb": "4.0.1",
    "node-cache": "^5.1.2",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "typedi": "0.8.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/agenda": "2.0.9",
    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/express-jwt": "6.0.1",
    "@types/helmet": "4.0.0",
    "@types/jest": "26.0.23",
    "@types/joi": "14.3.4",
    "@types/morgan": "1.9.3",
    "@types/node": "15.0.2",
    "@types/redis": "^2.8.32",
    "jest": "26.6.3",
    "nodemon": "2.0.12",
    "prettier": "2.3.0",
